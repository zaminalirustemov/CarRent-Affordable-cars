@model AboutViewModel
<section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('../assets/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
            <div class="col-md-9 ftco-animate pb-5">
                <p class="breadcrumbs"><span class="mr-2"><a href="index.html">Home <i class="ion-ios-arrow-forward"></i></a></span> <span>About us <i class="ion-ios-arrow-forward"></i></span></p>
                <h1 class="mb-3 bread">About Us</h1>
            </div>
        </div>
    </div>
</section>

<!-- ======= AboutUs Section ======= -->
@await Component.InvokeAsync("About",Model.AboutUs)
<!-- ======= Do You Want Section ======= -->
@await Component.InvokeAsync("DoYouWant")
<!-- ======= Testimonial Section ======= -->
@await Component.InvokeAsync("Testimonial")
<!-- ======= Info Section ======= -->
@await Component.InvokeAsync("Info")
